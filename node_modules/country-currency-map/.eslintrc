{
	/* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
	"extends": "defaults",
	"parser": "babel-eslint",
	"env": {
		"amd": true,
		"browser": true,
		"jquery": true,
		"node": true,
		"es6": true,
		"worker": true
	},
	"rules": {

		"eqeqeq": 2,
		"comma-dangle": 2,
		"no-console": 0,
		"no-debugger": 2,
		"no-extra-semi": 2,
		"no-extra-parens": [2, "functions"],
		"no-irregular-whitespace": 0,
		"no-undef": 0,
		"no-unused-vars": 0,
		"semi": 2,
		"semi-spacing": 2,
		"valid-jsdoc": [
			2,
			{ "requireReturn": false }
		]
	}
}
